{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "prefer-const": "off",
        "no-unused-vars": "off",
        "no-console": "off",
        "no-undef-init": "off",
        "no-plusplus": "off",
        "arrow-body-style": "off"
    }
}
