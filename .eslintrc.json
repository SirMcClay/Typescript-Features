{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"prefer-const": "off",
		"no-unused-vars": "off",
		"no-console": "off",
		"no-undef-init": "off",
		"no-plusplus": "off",
		"arrow-body-style": "off",
		"func-names": "off",
		"class-methods-use-this": "off",
		"max-classes-per-file": "off",
		"no-useless-constructor": "off",
		"no-empty-function": "off",
		"no-tabs": "off",
		"indent": "off",
		"no-new": "off",
		"no-use-before-define": "off",
		"no-unreachable": "off",
		"no-param-reassign": "off",
		"comma-dangle": "off"
	}
}
